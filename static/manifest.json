{
    "manifest_version": 3,
    "name": "Side Panel Content Extractor",
    "version": "0.1.0",
    "action": {},
    "permissions": [
        "storage",
        "tabs",
        "sidePanel",
        "webNavigation",
        "scripting"
    ],
    "host_permissions": [
        "https://gemini.google.com/*"
    ],
    "background": {
        "service_worker": "background.js",
        "type": "module"
    },
    "side_panel": {
        "default_path": "sidepanel.html"
    },
    "content_scripts": [
        {
            "matches": [
                "https://gemini.google.com/app/*"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_start"
        }
    ]
}